<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.geometry.Insets?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="600.0" fx:controller="com.example.yugioh.controllers.DeckSetController" xmlns="http://javafx.com/javafx/19">
    <GridPane hgap="5.0" vgap="5.0">
        <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
        <fx:include fx:id="mainDeck" source="Deck.fxml" />
        <fx:include fx:id="extraDeck" source="Deck.fxml" GridPane.rowIndex="1" />
        <fx:include fx:id="sideDeck" source="Deck.fxml" GridPane.rowIndex="2" />
        <padding>
            <Insets left="10.0" right="10.0" />
        </padding>
    </GridPane>
</AnchorPane>
